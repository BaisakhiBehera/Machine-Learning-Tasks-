   "source": [
    "##########PYTHON PROGRAMMING ASSIGNMENT\n",
    "## Q.4 :- Word counter\n",
    "S = input(\"enter a string\")\n",
    "s = S.split()\n",
    "d = {}\n",
    "for i in s:\n",
    "    if i in d:\n",
    "        d[i] += 1\n",
    "    else:\n",
    "        d[i] = 1\n",
    "for key, value in d.items():\n",
    "    print(f\"'{key}': {value}\")\n",
    "\n",
    "\n"
   ]
